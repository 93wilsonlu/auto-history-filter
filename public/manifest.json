{
    "manifest_version": 3,
    "name": "Auto history filter",
    "version": "0.1",
    "description": "A chrome extension which can filter history automatically",
    "background": {
        "service_worker": "./background.js"
    },
    "permissions": ["storage", "history", "contextMenus"],
    "author": "Wilson Lu",
    "homepage_url": "https://github.com/93wilsonlu/auto-history-filter",
    "short_name": "React Extension",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Auto history filter",
        "default_icon": {
            "16": "/images/react-logo-16.png",
            "32": "/images/react-logo-32.png",
            "48": "/images/react-logo-48.png",
            "128": "/images/react-logo-128.png"
        }
    },
    "icons": {
        "16": "/images/react-logo-16.png",
        "32": "/images/react-logo-32.png",
        "48": "/images/react-logo-48.png",
        "128": "/images/react-logo-128.png"
    }
}
